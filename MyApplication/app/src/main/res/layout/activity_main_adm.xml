<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/header_adm" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="63dp"
        android:layout_height="55dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView10"
        app:layout_constraintEnd_toEndOf="@+id/imageView10"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="@+id/imageView10"
        app:layout_constraintTop_toTopOf="@+id/imageView10"
        app:layout_constraintVertical_bias="0.36"
        app:srcCompat="@drawable/logohallel" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter_semibold"
        android:text="Bem vindo(a) ao perfil administrativo Hallel"
        android:textColor="@color/cortexto2"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView10"
        app:layout_constraintVertical_bias="0.052" />

    <Button
        android:id="@+id/btnmembros"
        android:layout_width="139dp"
        android:layout_height="122dp"
        android:layout_marginTop="48dp"
        android:background="@drawable/fundo_button_adm"
        android:drawableTop="@drawable/vector_membros"
        android:fontFamily="@font/inter_semibold"
        android:paddingTop="20dp"
        android:text="Membros"
        android:textColor="@color/cortexto2"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.143"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView29"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnAssociados"
        android:layout_width="139dp"
        android:layout_height="122dp"
        android:layout_marginTop="48dp"
        android:background="@drawable/fundo_button_adm"
        android:drawableTop="@drawable/vector_associados_adm_big"
        android:fontFamily="@font/inter_semibold"
        android:paddingTop="10dp"
        android:text="associados"
        android:textColor="@color/cortexto2"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.585"
        app:layout_constraintStart_toEndOf="@+id/btnmembros"
        app:layout_constraintTop_toBottomOf="@+id/textView29"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnFinanceiro"
        android:layout_width="139dp"
        android:layout_height="122dp"
        android:layout_marginBottom="236dp"
        android:background="@drawable/fundo_button_adm"
        android:drawableTop="@drawable/vector_cash"
        android:fontFamily="@font/inter_semibold"
        android:paddingTop="30dp"
        android:text="financeiro"
        android:textAlignment="center"
        android:textColor="@color/cortexto2"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.143"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnmembros"
        app:layout_constraintVertical_bias="0.969" />

    <Button
        android:id="@+id/btnEventos"
        android:layout_width="139dp"
        android:layout_height="122dp"
        android:background="@drawable/fundo_button_adm"
        android:drawableTop="@drawable/vector_eventos_adm"
        android:fontFamily="@font/inter_semibold"
        android:paddingTop="12dp"
        android:text="eventos"
        android:textColor="@color/cortexto2"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.585"
        app:layout_constraintStart_toEndOf="@+id/btnFinanceiro"
        app:layout_constraintTop_toBottomOf="@+id/btnAssociados"
        app:layout_constraintVertical_bias="0.211" />

    <Button
        android:id="@+id/btnAtividades"
        android:layout_width="139dp"
        android:layout_height="122dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/fundo_button_adm"
        android:drawableTop="@drawable/vector_atividades_adm"
        android:fontFamily="@font/inter_semibold"
        android:paddingTop="20dp"
        android:text="atividades"
        android:textColor="@color/cortexto2"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.143"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnFinanceiro"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnCursos"
        android:layout_width="139dp"
        android:layout_height="122dp"
        android:background="@drawable/fundo_button_adm"
        android:drawableTop="@drawable/vector_cursos_adm"
        android:fontFamily="@font/inter_semibold"
        android:paddingTop="20dp"
        android:text="cursos"
        android:textColor="@color/cortexto2"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.585"
        app:layout_constraintStart_toEndOf="@+id/btnAtividades"
        app:layout_constraintTop_toBottomOf="@+id/btnEventos"
        app:layout_constraintVertical_bias="0.551" />
</androidx.constraintlayout.widget.ConstraintLayout>